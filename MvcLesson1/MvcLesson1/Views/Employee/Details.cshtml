@model MvcLesson1.Models.Emploee

@{
    ViewBag.Title = "Employee Details";
}

<h2>Employee Details</h2>
<table>
    <thead>
        <tr>
            <th>Employee ID</th>
            <th>Name</th>
            <th>City</th>
        </tr>
    </thead>
    <tbody>
        <tr>
            <td>@Model.EmployeeId</td>
            <td>@Model.Name</td>
            <td>@Model.City</td>
        </tr>
    </tbody>
</table>